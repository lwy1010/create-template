{
  "name": "template-admin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "eslint": "eslint \"src/**/*.{vue,js,ts}\" --fix",
    "stylelint": "stylelint \"src/**/*.{vue,css,scss}\" --fix",
    "prettier": "prettier \"src/**/*.{js,ts,json,css,scss,vue,md}\" --write"
  },
  "dependencies": {
    "@7inch/storage": "^0.1.3",
    "@element-plus/icons-vue": "^0.2.4",
    "@vueuse/core": "^7.5.1",
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "element-plus": "^1.2.0-beta.6",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.9",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@7inch/eslint-config-vue": "^0.4.0",
    "@7inch/stylelint-config": "^1.0.0",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^17.0.9",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.24",
    "eslint": "^7.22.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.5.1",
    "sass": "^1.45.2",
    "stylelint": "^13.13.1",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.17.11",
    "vite": "^2.7.10",
    "vite-plugin-svg-icons": "^1.1.0",
    "vite-plugin-windicss": "^1.6.1",
    "vue-tsc": "^0.30.6",
    "windicss": "^3.4.2",
    "yorkie": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{vue,css,scss}": "stylelint --fix",
    "*.{js,ts,vue,scss,md}": "prettier --write"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint --edit $1"
  },
  "engines": {
    "node": "^14"
  },
  "license": "MIT"
}
