{
  "name": "template-admin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build && rimraf dist/**/*.js.map",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "eslint": "eslint \"src/**/*.{vue,js,ts}\" --fix",
    "stylelint": "stylelint \"src/**/*.{vue,css,scss}\" --fix",
    "prettier": "prettier \"src/**/*.{js,ts,json,css,scss,vue,md}\" --write"
  },
  "dependencies": {
    "@7inch/storage": "^0.1.4",
    "@element-plus/icons-vue": "^1.1.4",
    "@sentry/tracing": "^6.19.7",
    "@sentry/vue": "^6.19.7",
    "@vueuse/core": "^8.3.1",
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "element-plus": "^2.0.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.9",
    "vue": "^3.2.26",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@7inch/eslint-config-vue": "^0.9.0",
    "@7inch/stylelint-config": "^1.3.0",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^17.0.18",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/compiler-sfc": "^3.2.31",
    "eslint": "^7.22.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.45.2",
    "stylelint": "^13.13.1",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vite-plugin-sentry": "^1.0.14",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-windicss": "^1.8.4",
    "vue-tsc": "^0.31.4",
    "windicss": "^3.5.1",
    "yorkie": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{vue,css,scss}": "stylelint --fix",
    "*.{js,ts,vue,scss,md}": "prettier --write"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint --edit $1"
  },
  "engines": {
    "node": "^14"
  },
  "license": "MIT"
}
