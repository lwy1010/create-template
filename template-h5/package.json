{
  "name": "template-h5",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build --mode=production",
    "serve": "vite preview --host",
    "eslint": "eslint \"src/**/*.{vue,js,ts}\" --fix",
    "stylelint": "stylelint \"src/**/*.{vue,css,scss}\" --fix",
    "prettier": "prettier \"src/**/*.{js,ts,json,css,scss,vue,md}\" --write",
    "postinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "vant": "^3.2.1",
    "vconsole": "^3.8.1",
    "vue": "^3.2.6",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@7inch/eslint-config": "^0.1.0",
    "@7inch/stylelint-config": "^0.1.0",
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.20.0",
    "lint-staged": "^10.5.4",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "stylelint": "^13.11.0",
    "vite": "^2.4.2",
    "vite-plugin-style-import": "^1.0.0",
    "yorkie": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "@7inch/eslint-config"
  },
  "stylelint": {
    "extends": [
      "@7inch/stylelint-config"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{vue,css,scss}": "stylelint --fix",
    "*.{js,ts,vue,scss,md}": "prettier --write"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint --edit $1"
  },
  "engines": {
    "node": "^12"
  },
  "license": "MIT"
}
