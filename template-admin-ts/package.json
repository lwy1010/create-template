{
  "name": "template-admin-ts",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "eslint": "eslint \"src/**/*.{vue,js,ts}\" --fix",
    "stylelint": "stylelint \"src/**/*.{vue,css,scss}\" --fix",
    "prettier": "prettier \"src/**/*.{js,ts,json,css,scss,vue,md}\" --write"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "element-plus": "^1.0.2-beta.46",
    "js-cookie": "^2.2.1",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "vue": "^3.0.11",
    "vue-router": "^4.0.8",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@7inch/eslint-config": "^0.0.7",
    "@7inch/stylelint-config": "^0.1.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^15.6.1",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^7.22.0",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "prettier": "^2.3.0",
    "sass": "^1.34.1",
    "stylelint": "^13.12.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.6",
    "vite-plugin-mockit": "^1.0.2",
    "vite-plugin-svg-sprite-component": "^1.0.8",
    "yorkie": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "@7inch/eslint-config",
    "overrides": [
      {
        "files": [
          "mock/*.js"
        ],
        "rules": {
          "@typescript-eslint/no-var-requires": "off"
        }
      }
    ]
  },
  "stylelint": {
    "extends": [
      "@7inch/stylelint-config"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{vue,css,scss}": "stylelint --fix",
    "*.{js,ts,vue,scss,md}": "prettier --write"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint --edit $1"
  },
  "engines": {
    "node": "^12"
  },
  "license": "MIT"
}
