{
  "name": "template-rest-api",
  "version": "0.0.1",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register --transpile-only src/app.ts",
    "jobs": "node -r tsconfig-paths/register src/jobs/index.ts",
    "eslint": "eslint \"src/**/*.{js,ts}\" --fix",
    "prettier": "prettier \"src/**/*.{js,ts,json,md}\" --write"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.10",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "express": "^4.17.1",
    "joi": "^17.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "6.0.4",
    "mongoose-paginate-v2": "^1.4.1",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "@7inch/eslint-config-node": "^0.3.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^0.0.39",
    "@types/cors": "^2.8.12",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.172",
    "@types/mongoose-paginate-v2": "^1.3.11",
    "@types/superagent": "^4.1.12",
    "eslint": "^7.31.0",
    "lint-staged": "^11.1.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2",
    "yorkie": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "@7inch/eslint-config-node"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,md}": "prettier --write"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint --edit $1"
  },
  "engines": {
    "node": "^12"
  },
  "license": "MIT"
}
